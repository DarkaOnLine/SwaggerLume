{
  "name": "darkaonline/swagger-lume",
  "description": "Swagger integration to Lumen 5",
  "keywords": ["lumen", "laravel", "swagger"],
  "license": "MIT",
  "authors": [
    {
      "name": "Darius Matulionis",
      "email": "darius@matulionis.lt"
    }
  ],
  "require": {
    "php": ">=7.2",
    "laravel/lumen-framework": "~6.0|~7.0|^8.0",
    "zircote/swagger-php": "~2.0|3.*",
    "swagger-api/swagger-ui": "^3.0"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.8",
    "phpunit/phpunit": "8.*",
    "mockery/mockery": "1.*",
    "satooshi/php-coveralls": "^2.0",
    "vlucas/phpdotenv": "~3.3|~4.0"
  },
  "autoload": {
    "files": [
      "src/helpers.php"
    ],
    "psr-4": {
      "SwaggerLume\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
